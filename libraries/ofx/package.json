{
   "bugs": {
      "url": "https://github.com/F-O-T/montte-nx/issues"
   },
   "dependencies": {
      "zod": "4.1.13"
   },
   "description": "Typesafe ofx handling",
   "devDependencies": {
      "@biomejs/biome": "^2.3.7",
      "@types/bun": "1.3.3",
      "bumpp": "10.3.2",
      "bunup": "0.16.10",
      "simple-git-hooks": "2.13.1",
      "typescript": "5.9.3"
   },
   "exports": {
      ".": {
         "import": {
            "default": "./dist/index.js",
            "types": "./dist/index.d.ts"
         }
      },
      "./package.json": "./package.json"
   },
   "files": [
      "dist"
   ],
   "homepage": "https://github.com/F-O-T/montte-nx#readme",
   "license": "MIT",
   "module": "./dist/index.js",
   "name": "@fot/ofx",
   "peerDependencies": {
      "typescript": ">=4.5.0"
   },
   "peerDependenciesMeta": {
      "typescript": {
         "optional": true
      }
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/F-O-T/montte-nx.git"
   },
   "scripts": {
      "build": "bunup",
      "dev": "bunup --watch",
      "lint": "biome check .",
      "lint:fix": "biome check --write .",
      "postinstall": "bun simple-git-hooks",
      "release": "bumpp --commit --push --tag",
      "test": "bun test",
      "test:coverage": "bun test --coverage",
      "test:watch": "bun test --watch",
      "type-check": "tsc --noEmit"
   },
   "simple-git-hooks": {
      "pre-commit": "bun run lint && bun run type-check"
   },
   "type": "module",
   "types": "./dist/index.d.ts",
   "version": "1.1.0"
}
